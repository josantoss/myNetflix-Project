{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_KEY": "@react_app_api_key"
  },
  "build": {
    "env": {
      "REACT_APP_API_KEY": "8b2a4b8d3c1e4f5a6b7c8d9e0f1a2b3c"
    }
  }
}
